{
  "name": "group-scheduler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:core\" \"npm run dev:proxy\"",
    "dev:core": "next dev -p 3000",
    "dev:proxy": "local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@line/liff": "^2.18.2",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0"
  }
}
